            rdfs:range sven:Primitive ;
            owl:propertyChainAxiom ( sven:component
                                     sven:fieldOfView
                                   ) ;
            rdfs:comment "Le champ de vision du point de vue."@fr ,
                         "The field of view of the point of view."@en ;
